{
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
		"prettier",
		"unused-imports",
		"simple-import-sort"
	],
    "rules": {
        "prettier/prettier": "warn",
		"unused-imports/no-unused-imports": "warn",
		"simple-import-sort/imports": "warn",
		"simple-import-sort/exports": "warn"
    }
}